{
    "name": "shoesten-tag/log-http",
    "description": "Log HTTP requests and responses in Laravel applications using PHP attributes",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "logging",
        "http",
        "request-logging",
        "response-logging",
        "php-attributes",
        "http-interceptor",
        "api-logging",
        "debug",
        "monitoring",
        "middleware"
    ],
    "authors": [
        {
            "name": "roes10",
            "email": "didio50@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ShoestenTag\\LogHttp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShoestenTag\\LogHttp\\LogHttpServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.6"
    },
    "scripts": {
        "test": "pest",
        "test:coverage": "pest --coverage"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}